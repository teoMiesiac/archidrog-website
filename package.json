{
  "name": "gatsby-starter-typescript-plus",
  "description": "A starter kit for TypeScript-based Gatsby projects with sensible defaults.",
  "version": "1.0.0",
  "private": true,
  "author": "Resi Respati <resir014@gmail.com>",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "clean": "rimraf public",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "dev": "gatsby develop",
    "format": "prettier --write \"src/**/*.{ts,tsx,md}\"",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "start": "npm run dev",
    "test": "npm run type-check && npm run lint",
    "type-check": "tsc",
    "pre-storybook": "copy -r ./public/page-data/sq/d ./public/static",
    "storybook": "gatsby clean && npm run build && copy -r ./public/page-data/sq/d ./public/static && SET NODE_ENV=test& start-storybook -s ./public -p 6006",
    "storybookk": "SET NODE_ENV=test& start-storybook -s ./public -p 6006",
    "build-storybook": "SET NODE_ENV=production build-storybook -s public"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "@emotion/core": "^10.0.35",
    "@emotion/styled": "^10.0.12",
    "babel-plugin-styled-components": "^1.10.7",
    "classnames": "^2.2.6",
    "gatsby": "^2.24.67",
    "gatsby-background-image": "^1.1.2",
    "gatsby-image": "^2.4.20",
    "gatsby-plugin-canonical-urls": "^2.3.12",
    "gatsby-plugin-emotion": "^4.3.12",
    "gatsby-plugin-netlify-cms": "^4.3.17",
    "gatsby-plugin-netlify-cms-paths": "^1.3.0",
    "gatsby-plugin-react-helmet": "^3.3.12",
    "gatsby-plugin-sharp": "^2.6.38",
    "gatsby-plugin-styled-components": "^3.3.12",
    "gatsby-plugin-typescript": "^2.4.21",
    "gatsby-remark-copy-linked-files": "^2.3.18",
    "gatsby-remark-images": "^3.3.32",
    "gatsby-remark-prismjs": "^3.5.15",
    "gatsby-remark-responsive-iframe": "^2.4.16",
    "gatsby-remark-smartypants": "^2.3.12",
    "gatsby-source-filesystem": "^2.3.32",
    "gatsby-transformer-json": "^2.4.13",
    "gatsby-transformer-remark": "^2.8.37",
    "gatsby-transformer-sharp": "^2.5.16",
    "modern-normalize": "^0.5.0",
    "netlify-cms-app": "^2.13.2",
    "normalize.css": "^8.0.1",
    "polished": "^3.6.7",
    "prism-themes": "^1.1.0",
    "prismjs": "^1.16.0",
    "ramda": "^0.27.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-hook-form": "^6.12.2",
    "react-multi-carousel": "^2.5.5",
    "react-table": "^7.6.2",
    "react-transition-group": "^4.4.1",
    "reflexbox": "^4.0.6",
    "styled-components": "^5.2.0",
    "styled-system": "^5.1.5",
    "styled-theming": "^2.2.0",
    "typescript": "^3.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@sambego/storybook-state": "^2.0.1",
    "@storybook/addon-actions": "^6.0.25",
    "@storybook/addon-docs": "^6.0.25",
    "@storybook/addon-essentials": "^6.0.25",
    "@storybook/addon-knobs": "^6.0.25",
    "@storybook/addon-links": "^6.0.25",
    "@storybook/addon-storyshots": "^6.0.25",
    "@storybook/addon-viewport": "^6.0.25",
    "@storybook/addons": "^6.0.25",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react": "^6.0.25",
    "@types/classnames": "^2.2.8",
    "@types/node": "^12.12.62",
    "@types/ramda": "^0.27.20",
    "@types/react": "^16.9.50",
    "@types/react-dom": "^16.8.4",
    "@types/react-helmet": "^5.0.8",
    "@types/styled-components": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "^2.6.0",
    "@typescript-eslint/parser": "^2.6.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-remove-graphql-queries": "^2.9.20",
    "babel-preset-react-app": "^9.1.2",
    "eslint": "^6.6.0",
    "eslint-config-blvd": "^1.0.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.1",
    "gatsby-plugin-root-import": "^2.0.5",
    "gh-pages": "^2.0.1",
    "prettier": "^1.18.2",
    "prettier-eslint": "^11.0.0",
    "rimraf": "^2.6.3",
    "run-script-os": "^1.1.1"
  }
}
